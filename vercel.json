{
  "version": 2,
  "builds": [
    {
      "src": "frontend/package.json",
      "use": "@vercel/next",
      "config": {
        "buildCommand": "npm run build",
        "installCommand": "npm install",
        "distDir": "frontend/.next"
      }
    }
  ],
  "env": {
    "NEXT_PUBLIC_BACKEND_URL": "wss://pushup-production.up.railway.app",
    "SKIP_ENV_VALIDATION": "true"
  }
}
